{
    "name": "boltphp/core",
    "description": "Bolt framework Core",
    "keywords": ["bolt"],
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Kuhl",
            "email": "travis@kuhl.co"
        }
    ],
    "require": {
        "php": ">=5.4.0",

        "boltphp/handlebars.php": "v0.8.1",

        "symfony/class-loader": "2.4.*",
        "symfony/http-foundation": "2.4.*",
        "symfony/process": "2.4.*",
        "symfony/routing": "2.4.*",
        "symfony/property-access": "2.4.*",
        "symfony/finder": "2.4.*",
        "symfony/dom-crawler": "2.4.*",
        "symfony/css-selector": "2.4.*",
        "symfony/console": "2.4.*",
        "symfony/filesystem": "2.4.*",

        "doctrine/orm": "dev-master",
        "doctrine/dbal": "dev-master",
        "doctrine/cache": "dev-master",
        "doctrine/lexer": "1.0.*@dev",

        "patchwork/utf8": "1.1.*",
        "guzzle/guzzle": ">=3.7.0,<3.9.0",
        "kriswallsmith/assetic": "1.*"
    },
    "require-dev": {
        "aws/aws-sdk-php": "2.4.*",
        "composer/composer": "*",

        "phpunit/phpunit": "3.7.*",
        "phpunit/dbunit": "1.3.*",

        "sami/sami": "*",
        "leafo/lessphp": "*",
        "leafo/scssphp": "*",
        "natxet/cssmin": "*"
    },
    "autoload": {
        "psr-0": {
            "bolt": "src/"
        }
    },
    "minimum-stability": "dev"
}
